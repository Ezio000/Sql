create database Company
	use Company

create table Appartments (
	Type int not null primary key,
	code nvarchar(50) not null,
	price nvarchar(10)
)
create table Banks (
	id int not null primary key,
	name nvarchar(50) not null,
	country int not null,
	score nvarchar(100)
)
create table Payments (
	id int not null primary key,
	idtypepayments int not null,
	sum money,
	"Date" date not null,
	idtenant int not null,
	idbank int not null
)
alter table Payments
	add constraint "Banks to payments" foreign key(idbank) references Banks (id)
	alter table Payments
	add constraint "Tenants to payments" foreign key(idtenant) references Tenants(code)
	alter table Payments
	add constraint "Type of payments to payments" foreign key(idtypepayments) references "Type of payments"(id)
create table Position (
	id int not null primary key,
	name nvarchar(50) not null,
	description nvarchar(50) not null,
)
create table Tenants (
	code int not null primary key,
	"name" nchar(50) not null,
	Email nchar(50) not null,
	Street nchar(12) not null,
)
create table "Type of payments" (
id int not null primary key,
nameofpayment nvarchar(50) not null,
"description" nchar(12) not null,
)
create table Utilities (
id int not null primary key,
"Name description" nchar(50) not null,
)
create table test (id int PRIMARY KEY, name varchar (MAX), otd int, salary money);
insert into test values (1, 'Maria', 212, 15000),
                        (2, 'Nazar', 456, 16000),
						(3, 'Myroslav', 231, 15000),
						(4, 'Oleg', 547, 17000),
						(5, 'Ivan', 866, 18000),
						(6, 'Olena', 976, 16000),
						(7, 'Igor', 158, 17000),
						(8, 'Andriy', 051, 18000),
						(9, 'Sergiy', 432, 15000),
						(10, 'Uliana', 654, 16000);
Select * From test
Update test set salary = 15000
Select * From test
Delete from test where salary = 15000
Select Name, salary
From test
Order by otd Desc

